<h2>Sign Up</h2>

<form action="/process-signup" method="POST">
  <label>
    Full name:
    <input type="text" name="fullName" placeholder="Leia Organa" />
  </label>
  <br />

  <label>
    Email:
    <input type="email" name="email" placeholder="leia@resistance.com" />
  </label>
  <br />

  <label>
    Password:
    <input type="password" name="originalPassword" placeholder="It's a secret" />
  </label>
  <br />

  <button>Sign Up</button>
</form>

